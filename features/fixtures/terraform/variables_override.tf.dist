
variable "env" {
  type = "string"
  default = "dev"
}

variable "aws_account_id" {
  type = "string"
  default = "MyAwsAccountID"
}

variable "aws_access_key" {
  type = "string"
  default = "MyAwsAccessKey"
}

variable "aws_secret_key" {
  type = "string"
  default = "MyAwsSecretKey"
}

variable "aws_region" {
  type = "string"
  default = "us-west-2"
}
